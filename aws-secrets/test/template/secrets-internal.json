{
  "Conditions": {},
  "Mappings": {},
  "Outputs": {
    "TestExternalSecretArn": {
      "Value": {
        "Fn::ImportValue": {
          "Fn::Join": [
            "-",
            [
              {
                "Ref": "ExternalSecretsStack"
              },
              "TestExternalSecretArn"
            ]
          ]
        }
      }
    },
    "TestInternalSecretArn": {
      "Value": {
        "Ref": "TestInternalSecret"
      }
    }
  },
  "Parameters": {
    "ExternalSecretsStack": {
      "Type": "String"
    }
  },
  "Resources": {
    "TestInternalSecret": {
      "Properties": {
        "Name": {
          "Fn::Join": [
            "/",
            [
              {
                "Ref": "AWS::StackName"
              },
              "internal",
              "test"
            ]
          ]
        }
      },
      "Type": "AWS::SecretsManager::Secret"
    }
  }
}